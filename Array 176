#include <stdio.h>

int main() {

    int n, target, found = -1;

    scanf("%d", &n);

    int arr[n];

    for (int i = 0; i < n; i++)

        scanf("%d", &arr[i]);

    scanf("%d", &target);

    for (int i = 0; i < n; i++) {

        if (arr[i] == target) {

            found = i;

            break;

        }

    }

    if (found != -1)

        printf("Door Number is %03d-DN\n", found);

    else

        printf("-1\n");

    return 0;

}


   

