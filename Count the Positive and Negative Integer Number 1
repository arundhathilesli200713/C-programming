#include <stdio.h>

int main() {

    int n, i;

    scanf("%d", &n);  

    int num, positive = 0, negative = 0;

    for (i = 0; i < n; i++) {

        scanf("%d", &num);

        if (num > 0)

            positive++;

        else if (num < 0)

            negative++;

    }

    printf("Count of Positive Integer is %.2f\n", (float)positive);

    printf("Count of Negative Integer is %.2f\n", (float)negative);

    return 0;

}




