#include <stdio.h>
#include <ctype.h>

int main() {
    char ch;
    scanf(" %c", &ch);

    if (isalpha(ch)) {
        switch(tolower(ch)) {
            case 'a':
            case 'e':
            case 'i':
            case 'o':
            case 'u':
                printf("The Character %c is Vowel\n", ch);
                break;
            default:
                printf("The Character %c is Consonant\n", ch);
                break;
        }
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}



    

